;;; Emacs: this file uses -*- Scheme -*- syntax.

;;; Information about Chicken 5 egg for chicken-install.

((synopsis "Client library for the gemini:// protocol")
 (category net)
 (license "ISC")
 (author "Lassi Kortela")
 (dependencies (openssl "2.2.0") r7rs uri-generic)
 (distribution-files
  "gemini.sld"
  "gemini/client.sld"
  "gemini.egg"
  "gemini.release-info")
 (components
  (extension gemini
             (source "gemini.sld")
             (csc-options "-R" "r7rs" "-X" "r7rs"))
  (extension gemini.client
             (source "gemini/client.sld")
             (csc-options "-R" "r7rs" "-X" "r7rs")
             (component-dependencies gemini))))
